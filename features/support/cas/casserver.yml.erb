database:
  adapter: <%= active_record_adapter %>
  database: <%= tmpdir %>/casserver.db

authenticator:
  class: CASServer::Authenticators::SQL
  database:
    adapter: <%= active_record_adapter %>
    database: <%= users_database_filename %>
  user_table: users
  username_column: username
  password_column: password

default_locale: en

log:
  file: <%= tmpdir %>/casserver.log
  level: INFO

